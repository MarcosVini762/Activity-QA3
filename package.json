{
  "name": "activity-qa3",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --runInBand",
    "test:smoke": "jest --testPathPattern=smoke.test.js --runInBand",
    "test:coverage": "jest --coverage --runInBand",
    "test:coverage:html": "jest --coverage --reporters=default --reporters=jest-html-reporters",
    "test:watch": "jest --watch",
    "test:all": "jest --runInBand --verbose",
    "test:parallel": "jest --maxWorkers=50%",
    "test:parallel:full": "jest --maxWorkers=100%",
    "test:parallel:limited": "jest --maxWorkers=2",
    "test:parallel:coverage": "jest --maxWorkers=50% --coverage",
    "test:ci": "npm run lint && npm run test:coverage && npm run report",
    "report": "node Spotify-API-Tests/src/utils/logReportGenerator.js",
    "test:report": "npm test && npm run report",
    "test:parallel:report": "npm run test:parallel && npm run report",
    "open:report": "start Spotify-API-Tests/logs/report.html",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "pre-test": "npm run lint",
    "pre-commit": "npm run lint && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarcosVini762/Activity-QA3.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MarcosVini762/Activity-QA3/issues"
  },
  "homepage": "https://github.com/MarcosVini762/Activity-QA3#readme",
  "dependencies": {
    "ajv": "^8.17.1",
    "axios": "^1.13.2",
    "dotenv": "^17.2.3",
    "jest": "^30.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "eslint": "^9.39.1",
    "eslint-plugin-jest": "^29.2.1",
    "jest-html-reporters": "^3.1.7"
  }
}
